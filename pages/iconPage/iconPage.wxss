/* pages/iconPage/iconPage.wxss */
.icon-page {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-evenly;
  align-items: center;
}
.icon-page::after {
  content: '';
  flex: 1;
}

.icon-list {
  width: 33.333%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 20rpx;
  padding: 20rpx 0;
}